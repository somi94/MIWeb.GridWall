include: NodeTypes/*

prototype(Neos.NodeTypes:Page) < prototype(Neos.Neos:Page) {
    head.stylesheets.gridwall = Neos.Fusion:Template {
        templatePath = 'resource://MIWeb.Neos.GridWall/Private/Templates/NodeTypes/GridWall.html'
        sectionName = 'styles'
        node = ${node}
        @position = 'after site'
    }

    body.javascripts.gridwall = Neos.Fusion:Template {
        templatePath = 'resource://MIWeb.Neos.GridWall/Private/Templates/NodeTypes/GridWall.html'
        sectionName = 'scripts'
        node = ${node}
        @position = 'after site'
    }
}

page = Neos.NodeTypes:Page

default < page
